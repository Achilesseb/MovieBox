{
  "arrowParens": "avoid",
  "useTabs": false,
  "bracketSpacing": true,
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSameLine": false,
  "tabWidth": 2,
  "endOfLine": "lf",
  "importOrder": [
    "^./src/(?!utils|graphql).*$",
    "^./src/graphql/(.*)$",
    "^./src/utils/(.*)$",
    "^../",
    "^./"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}